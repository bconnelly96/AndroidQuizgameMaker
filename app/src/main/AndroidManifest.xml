<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.temple.quizgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".game_logic.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".game_ui.MainMenu" />
        <activity android:name=".game_ui.EndMenu" />
        <activity android:name=".game_ui.MultipleChoiceVisual" />
        <activity android:name=".game_ui.TrueFalseActivity" />
        <activity android:name=".game_ui.AnswerActivity"></activity>
    </application>

</manifest>